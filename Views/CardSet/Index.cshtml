@model IEnumerable<CardSet>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <a asp-action="Set" asp-controller="Card" asp-route-id="@item.CardSetId">@Html.DisplayFor(modelItem =>
                    item.Name)</a>
                </td>

                <td>
                    <a asp-action="Edit" asp-route-id="@item.CardSetId">Edit</a> |

                    <a asp-action="Delete" asp-route-id="@item.CardSetId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
